<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:mapbox="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.mapbox.maps.MapView
        android:id="@+id/mapView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        mapbox:mapbox_attributionEnabled="false"
        mapbox:mapbox_cameraZoom="14"
        mapbox:mapbox_compassEnabled="true"
        mapbox:mapbox_compassGravity="end|bottom"
        mapbox:mapbox_compassMarginRight="16dp"
        mapbox:mapbox_logoGravity="center_horizontal|bottom"
        mapbox:mapbox_scaleBarEnabled="false" />

</FrameLayout>